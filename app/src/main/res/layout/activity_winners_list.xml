<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    tools:context=".activities.Activity_Winner_List"
    android:orientation="vertical">

    <TextView
        android:id="@+id/main_LBL_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="24dp"
        android:textColor="#000000"
        android:textSize="23sp"
        android:text="Winner list" />

    <FrameLayout
        android:id="@+id/main_LAY_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_margin="24dp"
        android:background="#45454545"/>

    <TextView
        android:id="@+id/main_LBL_map_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:text="Map"
        android:textColor="#000000"
        android:textSize="15sp" />

    <FrameLayout
        android:id="@+id/main_LAY_map"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_margin="24dp"
        android:background="#45454545"/>

    <Button
        android:id="@+id/winner_list_BTN_start"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start new game" />


</LinearLayout>
